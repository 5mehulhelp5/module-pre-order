{
    "name": "o2ti/pre-order",
    "description": "O2TI - Admin Create Pre Order",
    "require": {
        "php": "~7.4.0||~8.1.0||~8.2.0||~8.3.0",
        "magento/framework": "^103.0.0",
        "magento/module-customer": "^103.0.0",
        "magento/module-sales": "^103.0.0",
        "magento/module-email": "^101.1.0",
        "ext-json": "*"
    },
    "type": "magento2-module",
    "authors": [
        {
            "name": "Bruno Elisei",
            "email": "brunoelisei@o2ti.com"
        }
    ],
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "O2TI\\PreOrder\\": ""
        }
    },
    "require-dev": {
        "magento/magento-coding-standard": "*",
        "phpmd/phpmd": "^2.13",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.6.8"
    },
    "scripts": {
        "post-install-cmd": "vendor/bin/phpcs --config-set installed_paths ../../..,../../phpcompatibility/php-compatibility/PHPCompatibility",
        "post-update-cmd": "vendor/bin/phpcs --config-set installed_paths ../../..,../../phpcompatibility/php-compatibility/PHPCompatibility"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        {
            "type": "composer",
            "url": "https://repo-magento-mirror.fooman.co.nz/"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": false,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
